{
	"name": "NewsTab",
	"author": "Pierre Rudloff",
	"url": "https://github.com/Archi-Strasbourg/mediawiki-news-tab",
	"description": "https://github.com/Archi-Strasbourg/mediawiki-news-tab",
	"version": "0.1.0",
	"license-name": "GPL-3.0",
	"type": "skin",
	"manifest_version": 1,
	"Hooks": {
		"SkinTemplateNavigation": [
			"NewsTab\\NewsTab::replaceTabs"
		],
		"ArticleViewHeader": [
			"NewsTab\\NewsTab::getInfobox"
		],
		"MagicWordwgVariableIDs": [
			"NewsTab\\NewsTab::registerMagicWord"
		],
		"ParserGetVariableValueSwitch": [
			"NewsTab\\NewsTab::getMagicWord"
		]
	},
	"AutoloadClasses": {
		"NewsTab\\NewsTab": "NewsTab.php"
	},
	"ExtensionMessagesFiles": {
		"NewsTabMagic": "NewsTab.i18n.magic.php"
	}
}
